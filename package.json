{
  "name": "mrz-detection",
  "version": "0.4.1",
  "description": "Detect Machine Readable Zones (MRZ) in images of identity documents",
  "main": "src/index.js",
  "files": [
    "src",
    "fontData/12x12/mrz/ocrb.json",
    "models"
  ],
  "browserify": {
    "transform": [
      "brfs",
      [
        "deassertify",
        {
          "nobundle": true
        }
      ],
      [
        "babelify",
        {
          "presets": [
            "module:metro-react-native-babel-preset"
          ]
        }
      ]
    ],
    "plugin": [
      "wasmify"
    ]
  },
  "scripts": {
    "eslint": "eslint src run",
    "eslint-fix": "npm run eslint -- --fix",
    "test": "run-s testonly eslint",
    "test-travis": "eslint src && jest --coverage && codecov",
    "testonly": "jest",
    "build-js": "browserify src/index.js --standalone mrz-detector > build/Mrz.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/image-js/mrz-detection.git"
  },
  "keywords": [],
  "author": "Jefferson Hern√°ndez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/image-js/mrz-detection/issues"
  },
  "homepage": "https://github.com/image-js/mrz-detection#readme",
  "jest": {
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.2.2",
    "brfs": "^2.0.2",
    "browserify": "^17.0.0",
    "codecov": "^3.0.4",
    "deassertify": "^0.1.2",
    "eslint": "^5.0.1",
    "eslint-config-cheminfo": "^1.18.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "jest": "^26.6.3",
    "make-promises-safe": "^1.1.0",
    "metro-react-native-babel-preset": "^0.64.0",
    "npm-run-all": "^4.1.2",
    "papaparse": "^5.3.0",
    "rimraf": "^2.6.2",
    "tableify": "^0.0.3"
  },
  "dependencies": {
    "babelify": "^10.0.0",
    "bson": "^2.0.8",
    "canvas": "^2.6.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^6.0.1",
    "hog-features": "^1.0.0",
    "image-js": "^0.21.0",
    "libsvm-js": "^0.2.1",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.range": "^3.2.0",
    "lodash.uniq": "^4.5.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "ml-array-mean": "^1.0.1",
    "ml-array-median": "^1.0.1",
    "ml-distance": "^2.1.1",
    "ml-kernel": "^2.3.4",
    "ml-matrix": "^5.0.1",
    "ml-param-grid": "^1.0.0",
    "mrz": "^3.1.0",
    "ocr-tools": "git+https://github.com/burdettadam/ocr-tools.git",
    "radians-degrees": "^1.0.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "transformation-matrix": "^1.12.0"
  }
}
